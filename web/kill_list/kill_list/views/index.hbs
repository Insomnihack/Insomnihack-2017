<div class="container">
    <div class="starter-template">
        <h1 class="white_text">Most wanted</h1>
        <p class="lead">TOP PRIORITY TARGETS FOR T-X TERMINATRIX<br>SHOULD BE CAPTURED DEAD OR ALIVE.</p>
    </div>

{{# if success }}
    <section class="alert alert-success" role="alert">
        {{# each success }}
            <strong>{{ this.msg }}</strong>
        {{/each}}
    </section>
{{/if}}

{{# if errors }}
<section class="alert alert-danger" role="alert">
    <ul>
        {{# each errors }}
            <li><strong>{{ this.msg }}</strong></li>
        {{/each}}
    </ul>
</section>
{{/if}}
    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
